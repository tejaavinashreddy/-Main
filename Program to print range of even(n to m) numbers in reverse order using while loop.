import java.util.*;
public class Main {
	public static void main(String[] args) {
		
		int m,n;
		
		Scanner sc=new Scanner(System.in);
		m=sc.nextInt();
		n=sc.nextInt();
		int i=n;
		System.out.print("even numbers between m and n in reverse order are");
		System.out.println("\n");
		while(i>=m)
		{
			
			if(i%2==0)
			
			System.out.print(i+" ");
			i--;
		}
	}
}
